file = {:C$\backslash$:/Users/shin/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Batterman, Ganguly, Harbin - 2015 - High resolution spatial and temporal mapping of traffic-related air pollutants.pdf:pdf},
issn = {16604601},
journal = {International Journal of Environmental Research and Public Health},
keywords = {Air pollution,Dispersion models,Human exposure,PM{\textless}inf{\textgreater}2.5{\textless}/inf{\textgreater},Traffic},
number = {4},
pages = {3646--3666},
