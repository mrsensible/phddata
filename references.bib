number = {4},
pages = {625},
publisher = {MDPI},
title = {{Characterization of Urban Subway Microenvironment Exposure- A Case of Nanjing in China}},
url = {https://pubmed.ncbi.nlm.nih.gov/30791659 https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6406341/},
