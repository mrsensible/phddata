issn = {00167185},
journal = {Geoforum},
keywords = {Environmental justice,Geographical Information Systems,Spatial externalities,Urban pollution,Vulnerable populations},
pages = {117--131},
title = {{Assessing environmental justice through potential exposure to air pollution: A socio-spatial analysis in Madrid and Barcelona, Spain}},
volume = {69},
