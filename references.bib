keywords = {Accuracy,Generalized additive model,Geographically weighted regression,Kriging,Meteorological station,Satellite imagery,Spline,Weather interpolation},
number = {9},
pages = {8639--8670},
title = {{An assessment of methods and remote-sensing derived covariates for regional predictions of 1 km daily maximum air temperature}},
